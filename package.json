{
  "name": "the-sane-gatsby",
  "version": "0.1.0",
  "description": "Sanity, Gatsby, and Netlify! Oh my!",
  "author": "RC Maples",
  "license": "MIT",
  "keywords": [
    "sanity",
    "gatsby",
    "netlify"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rcmaples/the-sane-gatsby.git"
  },
  "scripts": {
    "dev": "gatsby develop && cd ./sanity && npm run dev",
    "typecheck": "tsc --noEmit",
    "dev:gatsby": "gatsby develop",
    "start:gatsby": "gatsby develop",
    "build:gatsby": "gatsby build",
    "serve:gatsby": "gatsby serve",
    "clean:gatsby": "gatsby clean",
    "dev:sanity": "cd ./sanity && npm run dev",
    "start:sanity": "cd ./sanity && npm run start",
    "build:sanity": "cd ./sanity && npm run build",
    "deploy:sanity": "cd ./sanity && npm run deploy",
    "sanity:graphql": "cd ./sanity && npm run deploy-graphql",
    "test": "echo \\\"Error: no test specified\\\" && exit 1"
  },
  "dependencies": {
    "@mdx-js/react": "^3.0.1",
    "autoprefixer": "^10.4.19",
    "gatsby": "^5.13.5",
    "gatsby-plugin-image": "^3.13.1",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-mdx": "^5.13.1",
    "gatsby-plugin-postcss": "^6.13.1",
    "gatsby-plugin-sharp": "^5.13.1",
    "gatsby-plugin-sitemap": "^6.13.1",
    "gatsby-source-filesystem": "^5.13.1",
    "gatsby-source-sanity": "^7.9.1",
    "gatsby-transformer-sharp": "^5.13.1",
    "postcss": "^8.4.40",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "typescript": "^5.3.3"
  }  
}
